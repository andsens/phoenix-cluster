[Unit]
Description=Setup Cluster DNS
After=k3s@server.service k3s@agent.service apply-all-manifests.service
Before=k3s@server.target k3s@agent.target

[Install]
WantedBy=k3s@server.target k3s@agent.target

[Service]
Type=oneshot
ExecStart=/usr/local/lib/upkg/.upkg/home-cluster/workloads/coredns/bin/setup-cluster-dns
