[Unit]
Description=Pull external images to containerd
Before=apply-all-manifests.service
Requires=k3s@server.service k3s@agent.service
After=k3s@server.service k3s@agent.service

[Install]
WantedBy=k3s@server.service k3s@agent.service

[Service]
Type=oneshot
ExecStart=/usr/local/lib/upkg/.upkg/home-cluster/workloads/node/bin/pull-external-images
